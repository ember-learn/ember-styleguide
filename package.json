{
  "name": "ember-styleguide",
  "version": "2.1.0",
  "description": "Addon to help standardize the ember family of websites",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/ember-learn/ember-styleguide.git",
  "license": "MIT",
  "contributors": [
    "Sivakumar Kailasam <siva@rubberduck.engineering>",
    "Ember.JS Learning team"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:js": "$(npm bin)/eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "release": "np",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^2.0.0",
    "ember-bootstrap": "^1.2.0",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-sass": "^7.1.4",
    "ember-svg-jar": "^1.1.1",
    "ember-truth-helpers": "^2.0.0"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-a11y-landmarks": "0.0.4",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.18.1",
    "ember-cli-autoprefixer": "^0.7.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-mirage": "^0.3.4",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-stylelint": "^1.0.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-font-awesome": "^3.0.5",
    "ember-load-initializers": "^1.0.0",
    "ember-moment": "^7.6.0",
    "ember-native-dom-helpers": "^0.5.10",
    "ember-resolver": "^4.0.0",
    "ember-sinon": "^1.0.1",
    "ember-source": "~2.18.0",
    "eslint": "^4.18.1",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3",
    "normalize.css": "^4.1.1",
    "np": "*",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^0.8.0",
    "stylelint-scss": "^2.2.0"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
