{
  "name": "ember-styleguide",
  "version": "8.5.0",
  "description": "Addon to help standardize the ember family of websites",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://ember-styleguide.netlify.app/",
  "repository": "https://github.com/ember-learn/ember-styleguide.git",
  "license": "MIT",
  "contributors": [
    "Sivakumar Kailasam <siva@rubberduck.engineering>",
    "Ember.JS Learning team"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "bottled-ember build --ember-version 4.4 --deps field-guide,field-guide-default-template,ember-cli-fastboot,prember --links docs,public,docs-styles:app/styles --no-overlay --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "concurrently \"npm:start:*\" --restart-after 5000 --prefix-colors cyan,white,yellow",
    "start:tests": "ember serve -p 0",
    "start:docs": "bottled-ember serve --ember-version 4.4 --output-path dist-bottled-docs --deps field-guide,field-guide-default-template,ember-cli-fastboot,prember,@percy/cli,@percy/ember --links docs,public,percy-test:tests/acceptance,docs-styles:app/styles --cache-name docs --no-overlay",
    "test": "npm-run-all lint test:*",
    "test:docs": "bottled-ember test --ember-version 4.4 --output-path dist-bottled-docs --deps field-guide,field-guide-default-template,ember-cli-fastboot,prember,@percy/cli,@percy/ember --links docs,public,percy-test:tests/acceptance,docs-styles:app/styles --cache-name docs --no-overlay",
    "test:ember": "ember test",
    "ember-compatibility-tests": "ember try:all"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.2",
    "@glimmer/component": "^1.1.2",
    "broccoli-funnel": "^3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^5.1.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-concurrency": "^2.2.0",
    "ember-named-blocks-polyfill": "^0.2.5",
    "ember-svg-jar": "^2.3.3",
    "ember-test-waiters": "^2.1.3",
    "ember-truth-helpers": "^3.0.0",
    "lodash.get": "^4.4.2",
    "normalize.css": "^8.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "static-postcss-addon-tree": "^2.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.7.0",
    "@embroider/test-setup": "^1.6.0",
    "@glimmer/tracking": "^1.1.2",
    "@percy/cli": "^1.27.3",
    "@percy/ember": "^4.2.0",
    "@release-it-plugins/lerna-changelog": "^5.0.0",
    "babel-eslint": "^10.1.0",
    "bottled-ember": "^1.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-array-helper": "^5.1.0",
    "ember-auto-import": "^2.4.1",
    "ember-cli": "~4.4.1",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-fastboot": "^3.2.0-beta.5",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-netlify": "^0.4.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylelint": "^4.0.0",
    "ember-cli-terser": "^4.0.2",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-scroll": "^1.0.3",
    "ember-sinon": "^2.2.0",
    "ember-source": "~4.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.8.0",
    "ember-try": "https://git@github.com/ember-cli/ember-try.git#master",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "lint-to-the-future": "^2.0.0",
    "lint-to-the-future-ember-template": "^1.2.0",
    "lint-to-the-future-eslint": "^2.0.1",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.0.0",
    "prember": "^1.1.0",
    "prettier": "^2.6.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^15.5.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-strict-value": "^1.8.0",
    "stylelint-order": "^4.0.0",
    "webpack": "^5.72.1"
  },
  "engines": {
    "node": "18.* || >= 20"
  },
  "volta": {
    "node": "18.18.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release-it": {
    "plugins": {
      "@release-it-plugins/lerna-changelog": {
        "infile": "CHANGELOG.md",
        "launchEditor": true
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_AUTH"
    }
  }
}
