{
  "name": "ember-styleguide",
  "version": "3.1.1",
  "description": "Addon to help standardize the ember family of websites",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://ember-learn.github.io/ember-styleguide",
  "repository": "https://github.com/ember-learn/ember-styleguide.git",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "Sivakumar Kailasam <siva@rubberduck.engineering>",
    "Ember.JS Learning team"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "release": "np"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "ember-auto-import": "^2.7.0",
    "ember-bootstrap": "^5.1.1",
    "ember-cli-babel": "^7.26.10",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-sass": "^8.0.1",
    "ember-svg-jar": "^1.2.2",
    "ember-truth-helpers": "^2.1.0",
    "node-sass": "^9.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@fortawesome/ember-fontawesome": "^2.0.0",
    "@fortawesome/free-regular-svg-icons": "5.4.2",
    "@fortawesome/free-solid-svg-icons": "5.4.2",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-a11y-landmarks": "^1.0.1",
    "ember-angle-bracket-invocation-polyfill": "^1.3.0",
    "ember-cli": "~3.28.6",
    "ember-cli-addon-docs": "^6.0.2",
    "ember-cli-addon-docs-yuidoc": "^0.2.1",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-netlify": "^0.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylelint": "^2.2.0",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "^3.28.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-font-awesome": "^3.0.5",
    "ember-keyboard": "^8.2.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.8.0",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-sinon": "^2.2.0",
    "ember-source": "~3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.15.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.8",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-qunit": "^6.2.0",
    "loader.js": "^4.7.0",
    "normalize.css": "^8.0.0",
    "np": "*",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.17.2",
    "qunit-dom": "^1.6.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "pnpm": {
    "patchedDependencies": {
      "@fortawesome/ember-fontawesome@2.0.0": "patches/@fortawesome__ember-fontawesome@2.0.0.patch"
    }
  }
}
