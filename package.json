{
  "name": "ember-styleguide",
  "version": "2.6.0-0",
  "description": "Addon to help standardize the ember family of websites",
  "keywords": [
    "ember-addon"
  ],
  "homepage": "https://ember-learn.github.io/ember-styleguide",
  "repository": "https://github.com/ember-learn/ember-styleguide.git",
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "Sivakumar Kailasam <siva@rubberduck.engineering>",
    "Ember.JS Learning team"
  ],
  "scripts": {
    "build": "ember build",
    "changelog": "auto-changelog --template changelog.template --unreleased-only --prepend --load-github-issue-data --github-cache-dir .changelog",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "release": "np",
    "test:all": "ember try:each"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "broccoli-funnel": "^2.0.1",
    "broccoli-merge-trees": "^3.0.1",
    "ember-bootstrap": "^4.6.3",
    "ember-cli-babel": "^7.11.1",
    "ember-cli-google-fonts": "^2.16.2",
    "ember-cli-htmlbars": "^4.0.5",
    "ember-cli-sass": "^8.0.1",
    "ember-svg-jar": "^1.2.2",
    "ember-truth-helpers": "^2.1.0",
    "node-sass": "^6.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.0.0",
    "@fortawesome/ember-fontawesome": "^0.1.8",
    "@fortawesome/free-regular-svg-icons": "5.4.2",
    "@fortawesome/free-solid-svg-icons": "5.4.2",
    "@glimmer/component": "^1.0.4",
    "auto-changelog": "github:mansona/auto-changelog#epic",
    "babel-eslint": "^10.0.3",
    "broccoli-asset-rev": "^3.0.0",
    "ember-a11y-landmarks": "^1.0.1",
    "ember-cli": "~3.13.2",
    "ember-cli-autoprefixer": "^0.8.1",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-fastboot": "^2.2.3",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-moment-shim": "^3.5.0",
    "ember-cli-netlify": "^0.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-stylelint": "^2.2.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-fetch": "^8.0.4",
    "ember-font-awesome": "^3.0.5",
    "ember-load-initializers": "^2.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-moment": "^7.8.0",
    "ember-native-dom-helpers": "^0.6.2",
    "ember-qunit": "^4.5.1",
    "ember-resolver": "^5.3.0",
    "ember-sinon": "^2.2.0",
    "ember-source": "~3.13.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^1.14.0",
    "ember-try": "^1.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.1.0",
    "eslint-plugin-node": "^10.0.0",
    "field-guide": "^1.6.0",
    "field-guide-addon-docs-template": "^1.0.0",
    "loader.js": "^4.7.0",
    "normalize.css": "^8.0.0",
    "np": "*",
    "prember": "^1.0.5",
    "qunit-dom": "^0.9.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-declaration-strict-value": "^1.0.4",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.1"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "publishConfig": {
    "access": "public"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
