<footer class="es-footer" ...attributes>
  {{!--
    Pass footer properties to support
    <EsFooter @infoLinks={{someOtherLinks}} />
  --}}
  <EsFooterInfo @infoLinks={{this.infoLinks}} @socialLinks={{socialLinks}} />

  <EsFooterStatement @tagline={{tagline}} />

  <hr class="footer-spacer container py-0 my-3">

  <EsFooterContributions @contributorLinks={{contributorLinks}} />
</footer>