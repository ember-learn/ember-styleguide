<div class="cta-note">
  <div class="cta-note-body">
    <div class="cta-note-heading hide-in-percy">{{nameTitle}}</div>
    <div class="cta-note-message">
      {{#if hasBlock}}
        {{yield}}
      {{else}}
        {{defaultMessage}}
      {{/if}}
    </div>
  </div>
  <img src={{imageUrl}} role="presentation" alt="" class="hide-in-percy">
</div>